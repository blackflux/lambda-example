{
  "name": "lambda-example",
  "version": "0.0.0-development",
  "description": "Example project for lambda-tdd.",
  "main": "lib/handler.js",
  "scripts": {
    "test": "npm run clean && npm run gardener && npm run test-simple",
    "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "semantic-release": "npm run build-artifact && npm run clean-build && npx semantic-release",
    "clean": "rm -rf lib",
    "build": "npx babel src --out-dir lib --source-maps inline --copy-files --include-dotfiles",
    "clean-build": "npm run clean && npm run build",
    "test-simple": "nyc mocha \"./test/**/*.spec.js\"",
    "gardener": "node gardener",
    "build-artifact": "npm run clean-build && tar -zxvf $(npm pack) && rm *-0.0.0-development.tgz && cd package && tar -zcvf ../release.tar.gz . && cd .. && rm -rf package"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/simlu/lambda-example.git"
  },
  "author": "Lukas Siemon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simlu/lambda-example/issues"
  },
  "homepage": "https://github.com/simlu/lambda-example#readme",
  "devDependencies": {
    "app-root-path": "2.1.0",
    "babel-cli": "7.0.0-beta.3",
    "babel-eslint": "8.2.6",
    "babel-plugin-transform-flow-comments": "7.0.0-beta.3",
    "babel-preset-flow": "7.0.0-beta.3",
    "babel-register": "7.0.0-beta.3",
    "chai": "4.1.2",
    "coveralls": "3.0.2",
    "js-gardener": "1.30.3",
    "js-yaml": "3.12.0",
    "lambda-tdd": "1.21.1",
    "mocha": "5.2.0",
    "nock": "9.4.3",
    "nyc": "12.0.2",
    "semantic-release": "15.8.1"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/simlu/lambda-example/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">= 6.10.3"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "lambda-rollbar": "1.9.9",
    "lambda-serverless-api": "1.19.1",
    "moment-timezone": "0.5.21",
    "request": "2.88.0"
  },
  "peerDependencies": {},
  "keywords": [],
  "nyc": {
    "tempDirectory": "./coverage/.nyc_output",
    "report-dir": "./coverage",
    "check-coverage": true,
    "per-file": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "gardener.js",
      "node_modules/*",
      "coverage/*",
      "flow-typed/*",
      "lib/*"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-register"
    ],
    "extension": [],
    "cache": true,
    "all": true,
    "babel": true
  }
}
