{
  "name": "lambda-example",
  "version": "0.0.0-development",
  "description": "Example project for lambda-tdd.",
  "main": "lib/handler.js",
  "scripts": {
    "test": "npm run clean && npm run gardener && npm run test-simple",
    "coveralls": "node ./node_modules/coveralls/bin/coveralls.js < ./coverage/lcov.info",
    "semantic-release": "npm run clean-build && semantic-release",
    "clean": "rm -rf lib",
    "build": "npx babel src --out-dir lib --source-maps inline --copy-files --include-dotfiles",
    "clean-build": "npm run clean && npm run build",
    "test-simple": "nyc mocha --recursive",
    "gardener": "node gardener"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/simlu/lambda-example.git"
  },
  "author": "Lukas Siemon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simlu/lambda-example/issues"
  },
  "homepage": "https://github.com/simlu/lambda-example#readme",
  "devDependencies": {
    "app-root-path": "2.0.1",
    "chai": "4.1.2",
    "coveralls": "3.0.0",
    "js-gardener": "1.16.0",
    "js-yaml": "3.11.0",
    "lambda-tdd": "1.11.15",
    "mocha": "5.0.5",
    "semantic-release": "15.1.7",
    "nyc": "11.6.0",
    "babel-cli": "7.0.0-beta.3"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/simlu/lambda-example/blob/master/LICENSE"
    }
  ],
  "engines": {
    "node": ">= 6.10.3"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "lambda-rollbar": "1.9.1",
    "lambda-serverless-api": "1.12.9",
    "moment-timezone": "0.5.14",
    "request": "2.85.0"
  },
  "peerDependencies": {},
  "keywords": [],
  "nyc": {
    "tempDirectory": "./coverage/.nyc_output",
    "report-dir": "./coverage",
    "check-coverage": true,
    "per-file": false,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100,
    "include": [
      "**/*.js"
    ],
    "exclude": [
      "gardener.js",
      "node_modules/*",
      "coverage/*",
      "flow-typed/*",
      "lib/*"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "require": [
      "babel-register"
    ],
    "extension": [],
    "cache": true,
    "all": true,
    "babel": true
  }
}
